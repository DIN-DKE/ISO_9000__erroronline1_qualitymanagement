{
	"source": "ARTIKELMANAGER.CSV",
	"sourceformat": "(?:^\\\"|\\\")(.*?)(?:\\\"[\n\r]|\\\"$|\\\";)",
	"headerrow": 2,
	"destination": "E:\\Quality Management\\assistant\\library\\module.data\\stocklist.js",
	"destinationoutputstart" :"//this file was automatically created by <stocklist.exe>\n\nvar stocklist_data={content:",
	"destinationoutputend": "};",
	"readcolumns": {
		"Item-ID": {
			"fields": ["ID"],
			"translate": false
		},
		"Distributor": {
			"fields": ["DISTRIBUTOR"],
			"translate": false
		},
		"Article Name": {
			"fields": ["CAPTION"],
			"translate": false
		},
		"Order Number": {
			"fields": ["NUMBER"],
			"translate": false
		},
		"Package Size": {
			"fields": ["QUANTITY"],
			"translate": true
		},
		"In Stock": {
			"fields": ["STOCK"],
			"translate": false
		},
		"Additional Information": {
			"fields": ["INFORMATION", "MODEL_NAME", "SIZE_NAME", "COLOR_NAME"],
			"translate": false
		}
	},
	"static": {
		"Documents": "E:/DistributorDocuments"
	},
	"filter": {
		"ORDERSTOP": "true"
	},
	"translate": {
		"QUANTITY": {
			"1": "piece",
			"2": "pair",
			"4": "meter",
			"5": "set",
			"6": "liter",
			"7": "square metere",
			"8": "bag"
		}
	}
}